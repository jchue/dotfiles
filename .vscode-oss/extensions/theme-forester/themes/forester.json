{
    "name": "forester",
    "type": "dark",
    "$schema": "vscode://schemas/color-theme",
    "tokenColors": [
        {
            "settings": {
                "background": "#1e2026",
                "foreground": "#a8a397",
                "selectionForeground": "#a8a397",
                "findHighlight": "#ffe792",
                "findHighlightForeground": "#333333",
                "selectionBorder": "#a8a397",
                "bracketsForeground": "#a8a397",
                "bracketsOptions": "underline",
                "bracketContentsForeground": "#a8a397",
                "bracketContentsOptions": "underline",
                "tagsOptions": "stippled_underline"
            }
        },
        {
            "name": "Comment",
            "scope": "comment",
            "settings": {
                "foreground": "#414140"
            }
        },
        {
            "name": "String",
            "scope": "string",
            "settings": {
                "foreground": "#8d6939"
            }
        },
        {
            "name": "Number",
            "scope": "constant.numeric",
            "settings": {
                "foreground": "#8d6939"
            }
        },
        {
            "name": "Built-in constant",
            "scope": "constant.language",
            "settings": {
                "foreground": "#52645d"
            }
        },
        {
            "name": "User-defined constant",
            "scope": [
                "constant.character",
                "constant.other"
            ],
            "settings": {
                "foreground": "#52645d"
            }
        },
        {
            "name": "Variable",
            "scope": "variable",
            "settings": {
                "fontStyle": ""
            }
        },
        {
            "name": "Keyword",
            "scope": "keyword",
            "settings": {
                "foreground": "#754531"
            }
        },
        {
            "name": "Storage",
            "scope": "storage",
            "settings": {
                "fontStyle": "",
                "foreground": "#52645d"
            }
        },
        {
            "name": "Storage type",
            "scope": "storage.type",
            "settings": {
                "fontStyle": "",
                "foreground": "#424e5c"
            }
        },
        {
            "name": "Class name",
            "scope": "entity.name.class",
            "settings": {
                "fontStyle": "underline",
                "foreground": "#52645d"
            }
        },
        {
            "name": "Inherited class",
            "scope": "entity.other.inherited-class",
            "settings": {
                "fontStyle": "",
                "foreground": "#52645d"
            }
        },
        {
            "name": "Function name",
            "scope": "entity.name.function",
            "settings": {
                "fontStyle": "",
                "foreground": "#cab696"
            }
        },
        {
            "name": "Function argument",
            "scope": "variable.parameter",
            "settings": {
                "fontStyle": "",
                "foreground": "#ffffff"
            }
        },
        {
            "name": "Tag name",
            "scope": "entity.name.tag",
            "settings": {
                "fontStyle": "",
                "foreground": "#424e5c"
            }
        },
        {
            "name": "Tag attribute",
            "scope": "entity.other.attribute-name",
            "settings": {
                "fontStyle": "",
                "foreground": "#52645d"
            }
        },
        {
            "name": "Library function",
            "scope": "support.function",
            "settings": {
                "fontStyle": "",
                "foreground": "#8a3a3a"
            }
        },
        {
            "name": "Library constant",
            "scope": "support.constant",
            "settings": {
                "fontStyle": "",
                "foreground": "#52645d"
            }
        },
        {
            "name": "Library class/type",
            "scope": [
                "support.type",
                "support.class"
            ],
            "settings": {
                "fontStyle": "",
                "foreground": "#52645d"
            }
        },
        {
            "name": "Library variable",
            "scope": "support.other.variable",
            "settings": {
                "fontStyle": ""
            }
        },
        {
            "name": "Invalid",
            "scope": "invalid",
            "settings": {
                "background": "#664e4d",
                "foreground": "#cf433e",
                "fontStyle": ""
            }
        },
        {
            "name": "Invalid deprecated",
            "scope": "invalid.deprecated",
            "settings": {
                "background": "#664e4d",
                "foreground": "#cf433e"
            }
        },
        {
            "name": "JSON String",
            "scope": "meta.structure.dictionary.json string.quoted.double.json",
            "settings": {
                "foreground": "#8d6939"
            }
        },
        {
            "name": "diff.header",
            "scope": [
                "meta.diff",
                "meta.diff.header"
            ],
            "settings": {
                "foreground": "#414140"
            }
        },
        {
            "name": "diff.deleted",
            "scope": "markup.deleted",
            "settings": {
                "foreground": "#e61f44"
            }
        },
        {
            "name": "diff.inserted",
            "scope": "markup.inserted",
            "settings": {
                "foreground": "#5f5a2f"
            }
        },
        {
            "name": "diff.changed",
            "scope": "markup.changed",
            "settings": {
                "foreground": "#845d2b"
            }
        },
        {
            "scope": "constant.numeric.line-number.find-in-files - match",
            "settings": {
                "foreground": "#8FBE00A0"
            }
        },
        {
            "scope": "entity.name.filename.find-in-files",
            "settings": {
                "foreground": "#E6DB74"
            }
        },
        {
            "scope": "keyword.other",
            "settings": {
                "foreground": "#6e778e"
            }
        },
        {
            "scope": [
                "meta.property-value",
                "support.constant.property-value",
                "constant.other.color"
            ],
            "settings": {
                "foreground": "#8d6939"
            }
        },
        {
            "scope": "meta.structure.dictionary.json string.quoted.double.json",
            "settings": {
                "foreground": "#52645d"
            }
        },
        {
            "scope": "meta.structure.dictionary.value.json string.quoted.double.json",
            "settings": {
                "foreground": "#8d6939"
            }
        },
        {
            "name": "Function argument",
            "scope": "meta.property-name support.type.property-name",
            "settings": {
                "fontStyle": "normal"
            }
        },
        {
            "name": "Function argument",
            "scope": "meta.property-value punctuation.separator.key-value",
            "settings": {
                "foreground": "#ffffff"
            }
        },
        {
            "name": "Function argument",
            "scope": [
                "keyword.other.use",
                "keyword.other.function.use",
                "keyword.other.namespace",
                "keyword.other.new",
                "keyword.other.special-method",
                "keyword.other.unit",
                "keyword.other.use-as"
            ],
            "settings": {
                "foreground": "#424e5c"
            }
        },
        {
            "name": "Function argument",
            "scope": [
                "meta.use support.class.builtin",
                "meta.other.inherited-class support.class.builtin"
            ],
            "settings": {
                "foreground": "#424e5c",
                "fontStyle": "normal"
            }
        },
        {
            "name": "Function argument",
            "scope": "variable.other",
            "settings": {
                "foreground": "#ffffff",
                "fontStyle": "normal"
            }
        },
        {
            "name": "Coffeescript Function argument",
            "scope": "variable.parameter.function.coffee",
            "settings": {
                "foreground": "#8d6939",
                "fontStyle": ""
            }
        },
        {
            "name": "Markdown Titles",
            "scope": "entity.name.section.markdown",
            "settings": {
                "foreground": "#52645d"
            }
        },
        {
            "name": "Markdown Title Hash",
            "scope": "punctuation.definition.heading.markdown",
            "settings": {
                "foreground": "#754531"
            }
        },
        {
            "name": "Markdown Raw",
            "scope": "markup.raw.inline.markdown",
            "settings": {
                "foreground": "#8d6939"
            }
        },
        {
            "name": "Markdown bold stars",
            "scope": [
                "punctuation.definition.bold.markdown",
                "punctuation.definition.italic.markdown"
            ],
            "settings": {
                "foreground": "#754531"
            }
        },
        {
            "name": "Markdown link title braces",
            "scope": [
                "punctuation.definition.string.begin.markdown",
                "punctuation.definition.string.end.markdown"
            ],
            "settings": {
                "foreground": "#754531"
            }
        },
        {
            "name": "Markdown link braces",
            "scope": "punctuation.definition.metadata.markdown",
            "settings": {
                "foreground": "#754531"
            }
        },
        {
            "name": "Markdown link",
            "scope": [
                "markup.underline.link.markdown",
                "markup.underline.link.image.markdown",
                "meta.image.inline.markdown"
            ],
            "settings": {
                "foreground": "#424e5c",
                "fontStyle": ""
            }
        },
        {
            "name": "Markdown bold/italic",
            "scope": [
                "markup.bold.markdown",
                "markup.italic.markdown"
            ],
            "settings": {
                "foreground": "#424e5c"
            }
        },
        {
            "name": "Markdown bold/italic",
            "scope": "markup.italic.markdown",
            "settings": {
                "fontStyle": "italic"
            }
        },
        {
            "name": "Markdown bold/italic",
            "scope": "markup.bold.markdown",
            "settings": {
                "fontStyle": "bold"
            }
        },
        {
            "name": "Markdown pre",
            "scope": "markup.raw.block.markdown",
            "settings": {
                "foreground": "#664e4d"
            }
        },
        {
            "name": "GitGutter deleted",
            "scope": "markup.deleted.git_gutter",
            "settings": {
                "foreground": "#e61f44"
            }
        },
        {
            "name": "GitGutter inserted",
            "scope": "markup.inserted.git_gutter",
            "settings": {
                "foreground": "#5f5a2f"
            }
        },
        {
            "name": "GitGutter changed",
            "scope": "markup.changed.git_gutter",
            "settings": {
                "foreground": "#845d2b"
            }
        },
        {
            "name": "Template expressions.",
            "scope": "meta.template.expression",
            "settings": {
                "foreground": "#ffffff"
            }
        }
    ],
    "colors": {
        /**
         * Base colors
         */
        "foreground":"#a8a397",
        "focusBorder": "#2c3632",

        /**
         * Activity Bar
         * The Activity Bar is displayed either on the far left or right of the workbench and allows fast switching between views of the Side Bar.
         */
        "activityBar.background":"#1c1c1c",
        "activityBar.foreground":"#a8a397",
        "activityBarBadge.background":"#1c1c1c",
        "activityBarBadge.foreground":"#a8a397",

        /**
         * Editor colors
         */
        "editor.background": "#141414",
        "editor.foreground": "#a8a397",
        "editorLineNumber.foreground": "#363a3c",
        "editorLineNumber.activeForeground": "#a8a397",
        "editorGutter.background": "#141414",
        "editorGutter.addedBackground":"#5f5a2f",
        "editorGutter.modifiedBackground":"#845d2b",
        "editorIndentGuide.background": "#1c1c1c",
        "editorIndentGuide.activeBackground": "#414140",
        "editor.lineHighlightBackground": "#1d2021",

        /**
         * Editor Groups & Tabs
         * Editor Groups are the containers of editors. There can be many editor groups. A Tab is the container of an editor. Multiple Tabs can be opened in one editor group.
         */
        "editorGroupHeader.tabsBackground": "#1c1c1c",
        "tab.activeBackground": "#141414",
        "tab.activeForeground": "#a8a397",
        "tab.activeBorderTop": "#845d2b",
        "tab.inactiveBackground": "#1c1c1c",
        "tab.inactiveForeground": "#52645d",

        /**
         * Editor widget colors
         * The Editor widget is shown in front of the editor content. Examples are the Find/Replace dialog, the suggestion widget, and the editor hover.
         */
        "editorWidget.background": "#1c1c1c",
        "editorWidget.border": "#1c1c1c",

        /**
         * Git colors
         */
        "gitDecoration.modifiedResourceForeground":"#845d2b",
        "gitDecoration.untrackedResourceForeground":"#5f5a2f",
        "gitDecoration.ignoredResourceForeground":"#414140",

        /**
         * Input control
         * Colors for input controls such as in the Search view or the Find/Replace dialog.
         */
        "input.background": "#141414",
        "input.border": "#1c1c1c",
        "input.foreground": "#a8a397",
        "input.placeholderForeground": "#414140",

        /**
         * Lists and trees
         * Colors for list and trees like the File Explorer. An active list/tree has keyboard focus, an inactive does not.
         */
        "list.activeSelectionBackground": "#2c3632",
        "list.activeSelectionForeground": "#845d2b",
        "list.inactiveSelectionBackground": "#1d2021",
        "list.inactiveSelectionForeground": "#d8d3c7",
        "list.hoverBackground": "#1d2021",
        "list.hoverForeground": "#d8d3c7",

        /**
         * Panel colors
         * Panels are shown below the editor area and contain views like Output and Integrated Terminal.
         */
        "panel.background":"#1c1c1c",
        "panel.border":"#1c1c1c",
        "panelTitle.activeBorder":"#845d2b",

        /**
         * Scrollbar control
         */
        "scrollbarSlider.background": "#363a3c",
        "scrollbarSlider.hoverBackground": "#a8a397",
        "scrollbarSlider.activeBackground": "#d8d3c7",

        /**
         * Side Bar
         * The Side Bar contains views like the Explorer and Search.
         */
        "sideBar.background": "#1c1c1c",
        "sideBar.foreground": "#a8a397",
        "sideBarSectionHeader.background": "#1d2021",
        "sideBarSectionHeader.foreground": "#a8a397",
        "sideBarTitle.foreground": "#a8a397",

        /**
         * Status Bar colors
         * The Status Bar is shown in the bottom of the workbench.
         */
        "statusBar.background": "#2c3632",
        "statusBar.foreground": "#a8a397",
        "statusBar.noFolderBackground": "#253847",
    
        /**
         * Integrated Terminal colors
         */
        "terminal.background":"#141414",
        "terminal.foreground":"#a8a397",
        "terminal.ansiBlack":"#1d2021",
        "terminal.ansiRed":"#8a3a3a",
        "terminal.ansiGreen":"#5f5a2f",
        "terminal.ansiYellow":"#8d6939",
        "terminal.ansiBlue":"#424e5c",
        "terminal.ansiMagenta":"#3F3547",
        "terminal.ansiCyan":"#52645d",
        "terminal.ansiWhite":"#a8a397",
        "terminal.ansiBrightBlack":"#363a3c",
        "terminal.ansiBrightRed":"#927572",
        "terminal.ansiBrightGreen":"#a1a6a0",
        "terminal.ansiBrightYellow":"#cdc4b8",
        "terminal.ansiBrightBlue":"#636c75",
        "terminal.ansiBrightMagenta":"#9596a9",
        "terminal.ansiBrightCyan":"#52645d",
        "terminal.ansiBrightWhite":"#d8d3c7",

        "editorCursor.foreground": "#f8f8f0",
        "editorWhitespace.foreground": "#404451",
        "editor.selectionBackground": "#d3643b55",
        "badge.background":"#424e5c",
        "badge.foreground":"#ffffff",
        "editorGroup.background":"#25272f",
        "titleBar.activeBackground":"#15161b",
        "list.focusBackground":"#404451",
        "editorWhitespace.foreground":"#343842",
        "editorRuler.foreground":"#343842",
        "editorLink.activeForeground":"#424e5c",
        "inputOption.activeBorder":"#52645d",
        "input.activeBorder":"#424e5c",
        "progressBar.background":"#cab696",
        "button.background":"#52645d",
        "button.foreground":"#ffffff",
        "dropdown.background":"#191b20",
        "dropdown.border":"#191b20",
        "dropdown.foreground":"#ffffff",
        "editorGutter.deletedBackground":"#e61f44",
        "gitDecoration.deletedResourceForeground":"#e61f44",
        "gitDecoration.conflictingResourceForeground":"#9d37fc",
        "editorBracketMatch.background":"#424e5c44",
        "editorBracketMatch.border":"#52645d",
        "notification.background":"#292c34",
        "notification.foreground":"#ffffff",
        "notification.buttonBackground":"#424e5c",
        "notification.buttonForeground":"#ffffff",
        "notification.infoBackground":"#9d37fc",
        "notification.warningBackground":"#845d2b",
        "notification.errorBackground":"#e61f44",
        "notification.infoForeground":"#ffffff",
        "notification.warningForeground":"#ffffff",
        "notification.errorForeground":"#ffffff",
        "extensionButton.prominentBackground":"#424e5c",
        "extensionButton.prominentForeground":"#ffffff",
        "extensionButton.prominentHoverBackground":"#939aaa",
        "peekView.border":"#424e5c",
        "peekViewEditor.background":"#15161b",
        "peekViewEditorGutter.background":"#191b20",
        "peekViewResult.background":"#292c34",
        "peekViewTitle.background":"#191b20",
        "peekViewTitleLabel.foreground":"#ffffff",
        "peekViewTitleDescription.foreground":"#f7f7f7",
        "peekViewResult.selectionBackground":"#22252c",
        "peekViewResult.selectionForeground":"#ffffff",
        "peekViewResult.fileForeground":"#f5f5f5",
        "peekViewResult.lineForeground":"#e6e6e6",
        "peekViewEditor.matchHighlightBackground":"#52645d55",
        "peekViewResult.matchHighlightBackground":"#52645d55",
        "editor.hoverHighlightBackground":"#424e5c22",
        "editor.wordHighlightBackground":"#cab69622",
        "editor.wordHighlightStrongBackground":"#52645d22",
        "editorOverviewRuler.findMatchForeground":"#52645d55",
        "editorOverviewRuler.modifiedForeground":"#845d2b",
        "editorOverviewRuler.addedForeground":"#5f5a2f",
        "editorOverviewRuler.deletedForeground":"#e61f44",
        "editorOverviewRuler.errorForeground":"#e61f44",
        "editorOverviewRuler.warningForeground":"#845d2b",
        "editorOverviewRuler.infoForeground":"#9d37fc",
        "scrollbar.shadow":"#070809",
        "editorSuggestWidget.highlightForeground":"#424e5c",
        "editorSuggestWidget.foreground":"#ffffff",
        "editorSuggestWidget.selectedBackground":"#4b505f"
    }
}